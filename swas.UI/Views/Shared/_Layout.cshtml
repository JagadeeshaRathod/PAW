            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },

        })
            .then(response => {
                if (response.ok) {

                    console.log('Notification data retrieved successfully');


                    window.location.href = event.target.href;
                } else {

                    console.error('Failed to retrieve notification data');
                }
            })
            .catch(error => {

                console.error('Error:', error);
            });
    }
</script>

