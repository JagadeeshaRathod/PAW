            $('#dailyCounter').text('Visitors Today: ' + data.today);
            $('#monthlyCounter').text('Monthly: ' + data.currentMonth);
            $('#totalCounter').text('Total Visitors: ' + data.total);// Make sure 'today' matches the property name in HitCounterDaily
        });
    });
</script>

